{
    "name": "Naver Blog PC Web",
    "version": "0.3.2",
    "description": "데스크톱에서 네이버 블로그 들어갔을 때 모바일 페이지가 아닌 자동으로 PC버전으로 보여지게 하기.",
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "128": "favicon.png"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "favicon.png"
        }
    },
    "content_scripts": [{
        "matches": [
            "http://blog.naver.com/*",
            "http://m.blog.naver.com/*",
            "https://blog.naver.com/*",
            "https://m.blog.naver.com/*",
            "http://map.naver.com/*",
            "http://m.map.naver.com/*",
            "https://map.naver.com/*",
            "https://m.map.naver.com/*",
            "http://cafe.naver.com/*",
            "http://m.cafe.naver.com/*",
            "https://cafe.naver.com/*",
            "https://m.cafe.naver.com/*",
            "http://m.naver.com/",
            "https://m.naver.com/",
            "http://m.place.naver.com/*",
            "https://m.place.naver.com/*"
        ],
        "js": ["contentScript.js"],
        "run_at": "document_start",
        "all_frames": true
    }],
    "manifest_version": 3
}